<header class="topbar" data-navbarbg="skin6">
  <nav class="navbar top-navbar navbar-expand-md">
    <div class="navbar-header" data-logobg="skin6">
      <a class="nav-toggler waves-effect waves-light d-block d-md-none" href="javascript:void(0)"><i
              class="ti-menu ti-close"></i></a>
      <div class="navbar-brand">
        <b class="logo-icon">
           <%= image_tag asset_path('logo-icon.png') %>
        </b>
        <span class="logo-text">
          <%= image_tag asset_path('logo-text.png') %>
        </span>
      </div>
      <a class="topbartoggler d-block d-md-none waves-effect waves-light" href="javascript:void(0)"
        data-toggle="collapse" data-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><i
            class="ti-more"></i>
      </a>
    </div>
      <div class="navbar-collapse collapse" id="navbarSupportedContent">
        <ul class="navbar-nav float-left mr-auto ml-3 pl-1"></ul>
        <ul class="navbar-nav float-right">
          <li class="nav-item d-none d-md-block">
            <a class="nav-link" href="javascript:void(0)">
              <form>
                <div class="customize-input">
                  <input class="form-control custom-shadow custom-radius border-0 bg-white"
                      type="search" placeholder="Search" aria-label="Search">
                  <i class="form-control-icon" data-feather="search"></i>
                </div>
              </form>
            </a>
          </li>
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" href="javascript:void(0)" data-toggle="dropdown"
              aria-haspopup="true" aria-expanded="false">
              <%= image_tag asset_path('profile-pic'), alt: "user", class: "rounded-circle", width: "40"%>
              <span class="ml-2 d-none d-lg-inline-block"><span class="text-dark">
                <%= current_user.email %> (<%= current_user.role %> )</span> <i data-feather="chevron-down"
                class="svg-icon"></i></span>
            </a>
            <div class="dropdown-menu dropdown-menu-right user-dd animated flipInY">
              <%= link_to destroy_user_session_path, method: :delete, class: 'dropdown-item' do %>
                <i data-feather="power" class="svg-icon mr-2 ml-1"></i> Logout
              <% end %>
              <div class="dropdown-divider"></div>
              <div class="pl-4 p-3"><a href="javascript:void(0)" class="btn btn-sm btn-info">ViewProfile</a></div>
            </div>
          </li>
          <%= link_to "hii", params["hiii"] %>
        </ul>
      </div>
  </nav>
</header>
